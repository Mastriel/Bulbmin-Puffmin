{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "productName": "Bulbmin Control Panel",
  "mainBinaryName": "Bulbmin Control Panel",
  "version": "../../package.json",
  "identifier": "dev.mastriel",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Bulbmin Control Panel",
        "width": 800,
        "height": 600,
        "minWidth": 650,
        "minHeight": 450,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}